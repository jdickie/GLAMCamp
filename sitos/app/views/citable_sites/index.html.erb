<h1>Listing citable_sites</h1>

<table>
  <tr>
    <th>Domain</th>
    <th>Scraping algorithm</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @citable_sites.each do |citable_site| %>
  <tr>
    <td><%= citable_site.domain %></td>
    <td><%= citable_site.scraping_algorithm.name %></td>
    <td><%= link_to 'Show', citable_site %></td>
    <td><%= link_to 'Edit', edit_citable_site_path(citable_site) %></td>
    <td><%= link_to 'Destroy', citable_site, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

